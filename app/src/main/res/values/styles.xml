<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextAppearance.AppCompat.Title.Expanded" parent="TextAppearance.AppCompat.Title">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#fff</item>
    </style>
    <style name="TextAppearance.AppCompat.Title.Collapsed" parent="TextAppearance.AppCompat.Title">
